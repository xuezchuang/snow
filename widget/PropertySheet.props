    <LIB_DIR>..\Deps\Libs</LIB_DIR>
    <INCLUDE_DIR>..\Deps\Include</INCLUDE_DIR>
    <BIN_DIR>..\bin</BIN_DIR>
  </PropertyGroup>
  <PropertyGroup />
